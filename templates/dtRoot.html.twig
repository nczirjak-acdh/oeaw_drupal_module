<table id="example" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            
            {% for item in header %} 
                <th>{{ item }}</th>                    
            {% endfor %}
            
            <th>Details</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>  
            {% for item in result %}
            <tr>
                <td>{{ item.uri }}</td>
                <td>{{ item.title }}</td>
                <td><a href="{{ item.detail }}">Details</a></td>
                <td><a href="{{ item.edit }}">edit</a></td>
            </tr>
        {% endfor %}                                          
    </tbody>
</table>
  