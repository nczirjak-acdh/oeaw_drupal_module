
<table id="" class="display" cellspacing="0" width="100%">
   <thead>
        <tr>
            <th>Property</th>           
            <th>Value</th>            
        </tr>
    </thead>
    <tbody>  
            {% for item in result %}
            <tr>                
                
                <td>{{ item.property }}</td>
                <td>{{ item.value }}</td>                
            </tr>
        {% endfor %}                                          
    </tbody>        
</table>

{% if childResult %}    
<br/><br/>
<div class="title">
    <h2>Child Resource</h2>
    <br/>
</div>    
<table id="" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Title</th>           
            <th>Edit</th>
            <th>Fedora</th>
        </tr>
    </thead>       
    <tbody>
        {% for cItem in childResult %}
            <tr>                
                
                <td><a href="{{ cItem.detail }}">{{ cItem.title }}</a></td>
                <td width="50px"><a href="{{ cItem.edit }}"><center><i class="fa fa-pencil-square-o fa-2x"></i></center></a></td>
                <td width="50px"><a href="{{ cItem.uri }}" target="_blank"><center><i class="fa fa-database fa-2x" aria-hidden="true" ></i></center></a></td>
            </tr>
        {% endfor %}         
    </tbody>
</table>
    
{% endif  %}    