
<div class="vcenter">
    <h2 class="title">{{ editResData.title }} |  <a href="/oeaw_editing/{{ editResData.editUrl }}"><i class="fa fa-pencil-square-o fa-1x"></i></a>  </h2>    
</div>    

<table id="" class="display" cellspacing="0" width="100%">
   <thead>
        <tr>
            <th>Property</th>           
            <th>Value</th>            
        </tr>
    </thead>
    <tbody>  
            {% for item in result %}
            <tr>                
                <td>{{ item.property }}</td>
                <td>{{ item.value }}</td>                
            </tr>
        {% endfor %}                                          
    </tbody>        
</table>

{% if childResult %}    
<br/><br/>
<div class="title">
    <h2>Child Resource  |  <a href=""><i class="fa fa-plus-circle fa-1x" aria-hidden="true"></i></a></h2>
    <br/>
</div>    
<table id="" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Title</th>           
            {% if userid != 0  %}
                <th>Edit</th>
            {% endif %}                
            <th>Fedora</th>
        </tr>
    </thead>       
    <tbody>
        {% for cItem in childResult %}
            <tr>                
                
                <td><a href="{{ cItem.detail }}">{{ cItem.title }}</a></td>
                {% if userid != 0  %}
                    <td width="50px"><a href="{{ cItem.edit }}"><center><i class="fa fa-pencil-square-o fa-2x"></i></center></a></td>
                {% endif %}                
                <td width="50px"><a href="{{ cItem.uri }}" target="_blank"><center><i class="fa fa-database fa-2x" aria-hidden="true" ></i></center></a></td>
            </tr>
        {% endfor %}         
    </tbody>
</table>
    
{% endif  %} 
<div>
    <input type="button" value="<< Back" onclick="history.back(-1)" />
</div>